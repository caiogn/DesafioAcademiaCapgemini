 * 
 */
package cursojava;

import java.util.Scanner;

/**
 * @author Usuario
 *
 */
public class Atividade01java {

	public static void main(String[] args) {

		int a, b, c;
		Scanner entrada = new Scanner(System.in);
		a = entrada.nextInt();
		b = entrada.nextInt();

		while (a != 0 && b != 0)
			;
		c = 2 * a - b;
		System.out.println(c);

		a = entrada.nextInt();
		b = entrada.nextInt();
	}

}
