package cursojava;

public class Atv02 {

	public static void main(String[] args) {
      int vetor[] = {1, 15, 5, 4, 20};
      int maior = 0;
      
      for(int i=0; i<vetor.length; i++) {
    	  if(maior < vetor[i]) {
    		  maior = vetor[i];
    	  }
      }
      System.out.println("O maior valor Ã©: " + maior);

	}

}
